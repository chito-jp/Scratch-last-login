{
  "name": "Scratch Last login",
  "description": "You can write last login time on bio.",
  "version": "1.0",
  "manifest_version": 3,
  "host_permissions": [
      "https://scratch.mit.edu/*",
      "https://api.scratch.mit.edu/*"
    ],
  "content_scripts": [
    {
      "matches": ["https://scratch.mit.edu/*"],
      "js": ["set.js"]
    }
  ],
    "options_ui": {
        "page": "options.html"
    },
  "permissions": ["storage"],
  "icons":{"128": "128.png"}
}